<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibernate.Clientes" table="clientes">
    <id column="codigo" name="codigo" type="java.lang.Integer">
      <generator class="increment"/>
      <!--generator class="assigned"/-->
    </id>
    <property column="identificacion" name="identificacion" type="java.lang.String"/>
    <property column="nombres" name="nombres" type="java.lang.String"/>
    <property column="direccion" name="direccion" type="java.lang.String"/>
    <property column="telefono" name="telefono" type="java.lang.String"/>
    <property column="tipo" name="tipo" type="java.lang.String"/>
    <property column="estado" name="estado" type="java.lang.Boolean"/>
    <property column="ultimoacceso" name="ultimoacceso" type="timestamp"/>
    <property column="acceso" name="acceso" type="timestamp"/>
    <!--set name="tarjetasCollection" inverse="true" lazy="true"  table="Tarjetas"  fetch="select">
          <key>
              <column name="codigo" not-null="true" />
          </key>
          <one-to-many class="hibernate.Tarjetas" />
      </set-->
    <!--set name="tarjetasCollection" inverse="true">
             <key column="tarjeta"/>
             <one-to-many class="hibernate.Tarjetas"/>
        </set-->
    <!--set name="tarjetasCollection" table="tarjetas">
        <key column="tarjeta"/>
        <one-to-many class="hibernate.Tarjetas"/>
     </set-->
    <!--set name="tarjetasCollection" inverse="true" table="tarjetas" >
          <key>
              <column name="tarjeta" not-null="true" />
          </key>
          <one-to-many class="hibernate.Tarjetas" />
      </set-->
  </class>
</hibernate-mapping>
