<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibernate.Registro" table="registro">
    <id column="codigo" name="codigo" type="java.lang.Integer">
      <generator class="increment"/>
      <!--generator class="assigned"/-->
    </id>
    <property column="tarjeta" name="tarjeta" type="java.lang.String"/>
    <property column="ticket" name="ticket" type="java.lang.String"/>
    <property column="fechaini" name="fechaini" type="java.util.Date"/>
    <property column="fechafin" name="fechafin" type="java.util.Date"/>
    <property column="diferencia" name="diferencia" type="java.util.Date"/>
  </class>
</hibernate-mapping>
