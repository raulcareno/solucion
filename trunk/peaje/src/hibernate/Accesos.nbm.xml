<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibernate.Accesos" table="accesos">
    <id column="codigo" name="codigo" type="java.lang.Integer">
      <generator class="increment"/>
      <!--generator class="assigned"/-->
    </id>
    <property column="pantalla" name="pantalla" type="java.lang.String"/>
    <property column="agregar" name="agregar" type="java.lang.Boolean"/>
    <property column="modificar" name="modificar" type="java.lang.Boolean"/>
    <property column="eliminar" name="eliminar" type="java.lang.Boolean"/>
    <property column="ingresar" name="ingresar" type="java.lang.Boolean"/>
    <many-to-one column="perfil" name="perfil"/>
  </class>
</hibernate-mapping>
