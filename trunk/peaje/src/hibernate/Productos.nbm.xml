<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibernate.Productos" table="productos">
    <id column="codigo" name="codigo" type="java.lang.Integer">
      <generator class="increment"/>
      <!--generator class="assigned"/-->
    </id>
    <property column="descripcion" name="descripcion" type="java.lang.String"/>
    <property column="valor" name="valor" type="java.lang.Long"/>
    <property column="bien" name="bien" type="java.lang.Boolean"/>
   </class>
</hibernate-mapping>
    