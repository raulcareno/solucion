	<subDataset name="New Dataset 1">
		<queryString language="SQL">
			<![CDATA[SELECT codigomap, mat.estado, notas.materia, nota3,
COUNT(IF(nota3 >= (18.50),1,NULL)) S, COUNT(IF(nota3 >= (18.50),1,NULL)) * 100 / COUNT(nota3) pS,
COUNT(IF(nota3 > (16.50) AND nota3 <=18.50,1,NULL)) MB, COUNT(IF(nota3 > (16.50) AND nota3 <=18.50,1,NULL)) * 100 / COUNT(nota3)  pMB,
COUNT(IF(nota3 > (15.50) AND nota3 <=16.50,1,NULL)) B,COUNT(IF(nota3 > (15.50) AND nota3 <=16.50,1,NULL)) * 100 / COUNT(nota3)   pB,
COUNT(IF(nota3 > (12.50) AND nota3 <=15.50,1,NULL)) R,COUNT(IF(nota3 > (12.50) AND nota3 <=15.50,1,NULL))* 100 / COUNT(nota3)   pR,
COUNT(IF(nota3 > (0) AND nota3 <=12.50,1,NULL)) I,COUNT(IF(nota3 > (0) AND nota3 <=12.50,1,NULL)) * 100 / COUNT(nota3)   PI,
COUNT(nota3), AVG(nota3)
 FROM notas, materia_profesor, matriculas mat, estudiantes est
WHERE notas.materia =  materia_profesor.materia AND materia_profesor.curso = '224'  AND notas.matricula = mat.codigomat
AND est.codigoest = mat.estudiante AND matricula IN (SELECT codigomat FROM matriculas WHERE  curso  =  '224'
AND estado IN ('Matriculado','Recibir Pase','Emitir Pase','Retirado') )AND notas.disciplina = FALSE AND materia_profesor.seimprime = TRUE
GROUP BY materia
  ORDER BY materia_profesor.orden]]>
		</queryString>
