<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibernate.Tarifas" table="tarifas">
    <id column="codigo" name="codigo" type="java.lang.Integer">
      <generator class="increment"/>
      <!--generator class="assigned"/-->
    </id>
    <property column="nombre" name="nombre" type="java.lang.String"/>
    <property column="desde" name="desde" type="java.lang.Long"/>
    <property column="hasta" name="hasta" type="java.lang.Long"/>
    <property column="valor" name="valor" type="java.math.BigDecimal"/>
 </class>
</hibernate-mapping>