<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="hibernate.Global" table="global">
    <id column="codigo" name="codigo" type="java.lang.Integer">
      <generator class="increment"/>
      <!--generator class="assigned"/-->
    </id>
    <property column="nombre" name="nombre" type="java.lang.String"/>
    <property column="grupo" name="grupo" type="java.lang.String"/>
  </class>
</hibernate-mapping>
