<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="peajePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
  <class>hibernate.Registro</class>
    <class>hibernate.Factura</class>
    <class>hibernate.Productos</class>
    <class>hibernate.Detalle</class>
    <class>hibernate.Tarjetas</class>
    <class>hibernate.Tarifas</class>
    <class>hibernate.Clientes</class>
    <class>hibernate.Auditoria</class>
    <class>hibernate.Global</class>
    <class>hibernate.Usuarios</class>
    <class>hibernate.Empresa</class>
    <class>hibernate.Accesos</class>
    <class>camaraIP.ImagePanel</class>
    <class>hibernate.Tarifasdiarias</class>
    <class>hibernate.Control</class>
    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
      <property name="javax.persistence.jdbc.url" value=""/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value=""/>
    </properties>
  </persistence-unit>
</persistence>
