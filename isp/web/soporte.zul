                                        <bandbox style="background-color:transparent" value="Ingresado" disabled="true" readonly="true" width="90px"  id="bdEstado">
                                            <bandpopup>
                                                <vbox>
                                                       
                                                    <listbox  onSelect="bdEstado.value=self.selectedItem.label;bdEstado.closeDropdown(); habil(self.selectedItem.value);"   id="estadoSoporte"  >
                                                        <listitem label="Ingresado" selected="true" value="0" />
                                                        <listitem  label="Agendado" value="3" />
                                                        <listitem label="RE-Agendado" value="4" />
                                                        <listitem label="Pendiente" value="2" />
                                                        <listitem label="Resuelto" value="1" />
                                                     
                                                    </listbox> 
                                                </vbox>
                                            </bandpopup>
                                        </bandbox>  
                                    
                                    Actividad:
                                        <bandbox style="background-color:transparent" value="Asistencia" disabled="true" readonly="true" width="90px"  id="bdActividad">
                                            <bandpopup>
                                                <vbox>

                                                    <listbox   onSelect="bdActividad.value=self.selectedItem.label;bdActividad.closeDropdown();"   id="estadoActividad"  >
                                                        <listitem selected="true" label="Asistencia" value="1" />
                                                        <listitem label="Retiro de Equipos" value="2" />
                                                        <listitem label="Traslado" value="3" />
                                                        <listitem label="Llamada" value="4" />
                                                        <listitem label="Instalación" value="5" />
                                                    </listbox> 
                                                </vbox>
                                            </bandpopup>
                                        </bandbox>
                                    
                                    </span>
                                </row>
                                   <row>
                                    <span style="float:right"> No. Orden: </span>
                                    <span>   
                                        <checkbox visible ="false" id="generar" onCheck="bd.disabled = (!self.checked) " disabled="true"   />
                                        <intbox id="noorden" readonly="true"   /> 
                                        Fecha:
                                        <datebox readonly="true" disabled="true"  style="background:white;"  
                                    width="140px"  format="dd-MM-yyyy HH:mm"  onCreate="self.value = adm.Date()" id="fechaorden"/> 
                                        <span>Téc.Asignado:</span>
                                        <bandbox style="background-color:transparent" disabled="true" readonly="true" width="240px"  id="bd">
                                            <bandpopup>
                                                <vbox>
                                                       
                                                    <listbox   onSelect="bd.value=(self.selectedItem.value).getApellidos() +' '+(self.selectedItem.value).getNombres() ;bd.closeDropdown();"   id="empleadosList"  >
                                            
                                                        <listitem forEach="${empleadosEncontrados}"    value="${each}">
                                                            <listcell label="${each.apellidos}" />
                                                            <listcell label="${each.nombres}" />
                                                        </listitem>
                                                    </listbox> 
                                                </vbox>
                                            </bandpopup>
                                        </bandbox>
                                        
                                    </span>
                                </row>
                                  <row>
                                    <span style="float:right"> Escalar: </span>
                                    <span>   
                                        <checkbox visible ="false" id="escalar" onCheck="bdEs.disabled = (!self.checked) " disabled="true"   />
                                        <span>Empleado:</span>
                                        <bandbox style="background-color:transparent" disabled="true" readonly="true" width="240px"  id="bdEs">
                                            <bandpopup>
                                                <vbox>
                                                    <listbox   onSelect="bdEs.value=(self.selectedItem.value).getApellidos() +' '+(self.selectedItem.value).getNombres() ;bdEs.closeDropdown();"   id="empleadosListEs"  >
                                                        <listitem forEach="${empleadosEncontrados}"    value="${each}">
                                                            <listcell label="${each.apellidos}" />
                                                            <listcell label="${each.nombres}" />
                                                        </listitem>
                                                    </listbox> 
                                                </vbox>
                                            </bandpopup>
                                        </bandbox>
                                        
                                    </span>
                                </row>
                                
                                <row>
                                    <span style="float:right"> Comentario: </span>
                                    <textbox readonly="true" cols="82"  rows="4" id="observacion"/> 
                                </row>
                                <row>
                                    <span style="float:right"> Solución: </span>
                                    <textbox readonly="true" cols="82"  rows="4" id="solucion"/> 
                                </row>
                                
                                <row style="background:#CDDFF5">
                                    <span style="float:right"> 
                                        <label style="font-weight:bold">Atendido por:</label>  
                                    </span>
                                    <textbox id="atendidanombre" cols="32" style="background:transparent;border:0px" readonly="true" />        
                                </row>
                            </rows>
                        </grid>
                           
                        <span>
                            <button image="/images/guardar.gif" id="guardars" label="Guardar"  disabled="true"  onClick="guardars();modificars.disabled = true;"/>
                            <button  image="/images/nuevo.gif" id="agregars" label="Agregar" onClick="nuevos(new Soporte(0)); habilitar(false); identificacion.focus();guardars.disabled = false;modificars.disabled = true" />
                            <button  image="/images/editar.gif" id="modificars"  label="Modificar" disabled="true"  onClick="habilitar(false); observacion.focus();guardars.disabled = false;"/>                           
                        </span>		
                        <panel framable="true" visible="true"  title="HISTORIAL DE ATENCIONES"   height="200px" width="795px">
                            <panelchildren>
                            
                        
	   
                                <listbox mold="paging" rows="10" pageSize="10"  onSelect="nuevos(soporte.selectedItem.value);modificars.disabled = false" id="soporte" width="100%">
                                    <listhead>
                                        <listheader label="Fecha"/>
                                        <listheader label="Observación"/>
                                        <listheader label="Orden Generada"/>
                                        <listheader label="Registrador"/>
                                        <listheader label="Técnico"/>
                                        <listheader label="Estado"/>
                                    </listhead>
                                    <listitem forEach="${allEvents}" value="${each}">
                                        <listcell label="${each.fecha}" />
                                        <listcell label="${each.observacion}" />
                                        <listcell label="${each.generada}" />
                                        <listcell label="${each.empleados}" />
                                    </listitem>
                                </listbox>
                            </panelchildren>	 
                        </panel>
                    </groupbox>
                        
                </tabpanel>
                <tabpanel>
         
                    <listbox mold="paging" rows="3" pageSize="3"   id="facturasDatos" width="100%">
                        <listhead>
                                   
                            <listheader label="Fecha Pago"/>
                            <listheader label="Servicio"/>
                            <listheader label="Total"/>
                            <listheader label="Saldo"/>
                            <listheader label="Opcion"/>
                        </listhead>
                        <listitem forEach="${allEvents}" value="${each}">
                        </listitem>
                    </listbox>    
                         
                       
                </tabpanel>
                <tabpanel>
                   
                    <listbox mold="paging" rows="3" pageSize="3" id="contratosCliente" height="100%" width="100%">
                        <listhead>
                            <listheader label="Plan"/>
                            <listheader label="Fecha"/>
                            <listheader label="Estado"/>
                        </listhead>
                        <listitem forEach="${allEvents}" value="${each}">
                            <listcell label="${each.fecha}" />
                        </listitem>
                    </listbox>
                       
                </tabpanel>
            </tabpanels>
        </tabbox>
        
        <grid  width="802px">
            <rows>
                <row>
                                        
                                 
              
                    <span>
                    
                        
                    </span>
                </row>
               
            </rows>
        </grid>

     
        <panel height="1px" visible="false" style="margin-bottom:10px"
                           title="Reporte" border="normal">
            <panelchildren>
                <jasperreport id="reportelocal" />
            </panelchildren>
        </panel>
             
     
    </div>
</window>
