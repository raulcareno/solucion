                                <button  id="reducir" label="Disminuir(-)" onClick="reducir()"/>
                                <button  id="aumentar" label="Aumentar (+)" onClick="aumentar()"/>
                                <button label="Refrescar...!" image="/images/actualizar.png" onClick='showReport();'/>
                            </span>

